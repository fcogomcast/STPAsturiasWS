    <!--  CRUBENCVS 46939 08/03/2023 -->
    <wsdl:operation name="AltaRemesaEnviosConjunto">
      <wsdl:input message="tns:AltaRemesaEnviosConjuntoRequest"/>
      <wsdl:output message="tns:AltaRemesaEnviosConjuntoResponse"/>
