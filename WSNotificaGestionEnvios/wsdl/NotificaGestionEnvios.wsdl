      </wsdl:output>
    </wsdl:operation>
    <wsdl:operation name="SincronizarEnvio">
      <soap:operation soapAction="http://notificagestionenvios.stpa.es/SincronizarEnvio"/>
      <wsdl:input>
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output>
        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
    <wsdl:operation name="ActualizarRespuesta">
      <soap:operation soapAction="http://notificagestionenvios.stpa.es/ActualizarRespuesta"/>
      <wsdl:input>
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output>
        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
    <!--  CRUBENCVS 46939 08/03/2023 -->
    <wsdl:operation name="AltaRemesaEnviosConjunto">
      <soap:operation soapAction="http://notificagestionenvios.stpa.es/AltaRemesaEnviosConjunto"/>
      <wsdl:input>
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output>
        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
    <!-- FIN CRUBENCVS 46939 08/03/2023 -->
  </wsdl:binding>
  <wsdl:service name="NotificaGestionEnvios">
    <wsdl:port binding="tns:NotificaGestionEnviosSOAP" name="NotificaGestionEnviosSOAP">
      <soap:address location="http://bus:7101/WSNotifica/ProxyServices/PXGestionEnvios"/>
    </wsdl:port>
  </wsdl:service>
